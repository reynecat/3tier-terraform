# Azure 인증 정보
subscription_id = "your-azure-subscription-id"
tenant_id       = "your-azure-tenant-id"

# Storage Account 설정
storage_account_name = "drbackupprod2"  # 전역 고유 이름 (소문자+숫자, 3-24자)

# Route53 설정 (옵션 - 도메인이 있는 경우)
enable_route53  = true                     # true로 변경하여 활성화
aws_region      = "us-east-1"
domain_name     = "example.com"             # Route53 호스팅 존 도메인
subdomain_name  = "maintenance.example.com" # 점검 페이지 서브도메인

# 환경 설정 (선택사항)
environment = "blue"
location    = "koreacentral"
